@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<h4>User info</h4>
<table class="table table-striped">
    <thead>
    <tr>
        <th>Type</th>
        <th>Value</th>
        <th>ValueType</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>
                @claim.Type
            </td>
            <td>
                @claim.Value
            </td>
            <td>
                @claim.ValueType
            </td>
            
        </tr>
    }
    </tbody>
</table>
